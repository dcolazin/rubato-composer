				<scope>provided</scope>
		    </dependency>
		<dependency>
		    <groupId>org.rubato.jars</groupId>
		    <artifactId>collections-generic</artifactId>
			<version>0.1.0-MAVEN</version>
		</dependency>
		<dependency>
		    <groupId>org.rubato.jars</groupId>
