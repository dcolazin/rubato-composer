		</dependency>
		<dependency>
		    <groupId>org.rubato.jars</groupId>
		    <artifactId>jung-api</artifactId>
			<version>0.1.0-MAVEN</version>
		</dependency>
		<dependency>
		    <groupId>org.rubato.jars</groupId>
		    <artifactId>jung-graph-impl</artifactId>
			<version>0.1.0-MAVEN</version>
		</dependency>
		<dependency>
		    <groupId>org.rubato.jars</groupId>
		    <artifactId>jung-visualization</artifactId>
			<version>0.1.0-MAVEN</version>
		</dependency>
		<dependency>
		    <groupId>org.rubato.jars</groupId>
		    <artifactId>junit</artifactId>
			<version>0.1.0-MAVEN</version>
			<scope>test</scope>
		</dependency>
		<dependency>
		    <groupId>org.rubato.jars</groupId>
		    <artifactId>LeapJava</artifactId>
			<version>0.1.0-MAVEN</version>
		</dependency>
		<dependency>
		    <groupId>org.rubato.jars</groupId>
		    <artifactId>mt4j</artifactId>
			<version>0.1.0-MAVEN</version>
		</dependency>
		<dependency>
		    <groupId>org.rubato.jars</groupId>
		    <artifactId>xercesImpl</artifactId>
			<version>0.1.0-MAVEN</version>
		</dependency>
	</dependencies>
	<dependencyManagement><!-- TODO --></dependencyManagement>
	<modules><!-- TODO --></modules>
	<properties>
	    <maven.compiler.source>1.6</maven.compiler.source>
	    <maven.compiler.target>1.6</maven.compiler.target>
		<project.encoding>utf-8</project.encoding>
	</properties>


	<!-- Build Settings -->
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-javadoc-plugin</artifactId>
				<version>3.4.0</version>
				<configuration>
					<source>1.6</source>
					<encoding>utf-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<version>2.4</version>
				<configuration>
					<archive>
						<manifest>
						    <addClasspath>true</addClasspath>
						    <mainClass>org.rubato.composer.Composer</mainClass>
						</manifest>
					</archive>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>2.7</version>
				<configuration>
				    <encoding>${project.encoding}</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-shade-plugin</artifactId>
				<version>3.2.4</version>
				<executions>
				  <execution>
				    <phase>package</phase>
				    <goals>
				      <goal>shade</goal>
				    </goals>
					<configuration>
                        <filters>
                            <filter>
                                <artifact>*:*</artifact>
                                <excludes>
                                    <exclude>META-INF/*.SF</exclude>
                                    <exclude>META-INF/*.DSA</exclude>
                                    <exclude>META-INF/*.RSA</exclude>
                                </excludes>
                            </filter>
                        </filters>
					</configuration>
				  </execution>
				</executions>
		      </plugin>
		</plugins>	
	</build>

